{
    "name":        "swarrot/swarrot",
    "description": "A simple lib to consume RabbitMQ queues",
    "keywords": ["swarrot","worker"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/options-resolver": "~2.3",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/prophecy-phpunit": "~1.0",
        "videlalvaro/php-amqplib": "~2.0",
        "doctrine/common": "~2.3",
        "doctrine/dbal": "~2.0"
    },
    "suggest": {
        "pecl-amqp": "*",
        "videlalvaro/php-amqplib": "~2.0",
        "symfony/console": "~2.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Swarrot": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
